{"components/common/alert/alert-modal":"<div id=\"alert-modal\" class=\"modal modal--alert modal--small\" data-reveal data-prevent-quick-search-close>\n    <div class=\"alert-icon error-icon\">\n        <span class=\"icon-content\">\n            <span class=\"line line-left\"></span>\n            <span class=\"line line-right\"></span>\n        </span>\n    </div>\n\n    <div class=\"alert-icon warning-icon\">\n        <div class=\"icon-content\">!</div>\n    </div>\n\n    <div class=\"modal-content\"></div>\n\n    <div class=\"button-container\">\n        <button type=\"button\" class=\"confirm button\" data-reveal-close>{{lang 'common.ok'}}</button>\n        <button type=\"button\" class=\"cancel button\" data-reveal-close>{{lang 'common.cancel'}}</button>\n    </div>\n</div>\n","components/common/body":"<main class=\"body\" id=\"main-content\" role=\"main\" data-currency-code=\"{{currency_selector.active_currency_code}}\">\r\n    {{#block \"hero\"}} {{/block}}\r\n    <div class=\"main-page-container\">\r\n        {{#block \"page\"}} {{/block}}\r\n    </div>\r\n    {{> components/common/modal/modal}}\r\n    {{> components/common/alert/alert-modal}}\r\n</main>\r\n","components/common/breadcrumbs":"<nav aria-label=\"Breadcrumb\">\r\n    <div class=\"container\">\r\n    <ol class=\"breadcrumbs\">\r\n        {{#unless theme_settings.hide_breadcrumbs }}\r\n            {{#each breadcrumbs}}\r\n                <li class=\"breadcrumb {{#if @last}}is-active{{/if}}\">\r\n                    <a class=\"breadcrumb-label\"\r\n                       href=\"{{url}}\"\r\n                       {{#if @last}}aria-current=\"page\"{{/if}}\r\n                    >\r\n                        <span>{{name}}</span>\r\n                    </a>\r\n                </li>\r\n            {{/each}}\r\n        {{/unless}}\r\n    </ol>\r\n    </div>\r\n</nav>\r\n\r\n<script type=\"application/ld+json\">\r\n{\r\n    \"@context\": \"https://schema.org\",\r\n    \"@type\": \"BreadcrumbList\",\r\n    \"itemListElement\":\r\n    [\r\n        {{#each breadcrumbs}}\r\n        {\r\n            \"@type\": \"ListItem\",\r\n            \"position\": {{add @index 1}},\r\n            \"item\": {\r\n                \"@id\": {{{JSONstringify url}}},\r\n                \"name\": {{{JSONstringify name}}}\r\n            }\r\n        }{{#unless @last}},{{/unless}}\r\n        {{/each}}\r\n    ]\r\n}\r\n</script>\r\n","components/common/footer":"<footer class=\"footer\" role=\"contentinfo\">\r\n    {{{region name=\"footer_custom--global\"}}}\r\n</footer>\n","components/common/header":"<a href=\"#main-content\" class=\"skip-to-main-link\">{{lang 'header.skip_to_main'}}</a>\r\n<header class=\"header sticky\" role=\"banner\">\r\n    {{> components/common/navigation}}\r\n    <div class=\"container\">\r\n        <div class=\"header-container\">\r\n            <div class=\"logo-container\">\r\n                {{> components/common/store-logo}}\r\n            </div>\r\n            <div class=\"search-container\">\r\n               <form class=\"form\"  action=\"{{urls.search}}\">\r\n                    <fieldset class=\"form-fieldset\">\r\n                        <div class=\"form-field header-form-field\">\r\n                            <label class=\"form-label is-srOnly\" for=\"search_query_adv\">{{lang 'search.results.form_label'}}</label>\r\n                            <div class=\"form-prefixPostfix wrap\">\r\n                                <input class=\"magnifiying-glass\" type=\"submit\" value=\"{{lang 'search.results.form_button_text'}}\">\r\n                                <input class=\"form-input\" id=\"search_query_adv\" name=\"search_query_adv\" value=\"{{forms.search.query}}\" placeholder=\"Search\">\r\n                            </div>\r\n                        </div>\r\n                    </fieldset>\r\n                </form>\r\n            </div>\r\n            <div class=\"main-menu-container\">\r\n                <div class=\"link-list-menu-container\">\r\n                    {{> components/common/menu-navigation_}}\r\n                </div>\r\n                <div class=\"icon-menu-container\">\r\n                    <ul class=\"ul-icon-menu navUser-section navUser-section--alt\">\r\n                        <li class=\"navUser-item navUser-item--account\">\r\n                            {{#if customer}}\r\n                            <a class=\"header-icon-container\" href=\"{{urls.account.index}}\"\r\n                                aria-label=\"{{lang 'common.account'}}\">\r\n                                <span class=\"svg-container account-svg\"><svg xmlns=\"http://www.w3.org/2000/svg\" width=\"21\" height=\"24\" viewBox=\"0 0 21 24\">\r\n                              <path id=\"Path_116\" data-name=\"Path 116\" d=\"M10.5-9a6,6,0,0,0,6-6,6,6,0,0,0-6-6,6,6,0,0,0-6,6A6,6,0,0,0,10.5-9Zm2.377,2.25H8.123A8.124,8.124,0,0,0,0,1.373,1.625,1.625,0,0,0,1.625,3H19.376A1.622,1.622,0,0,0,21,1.373,8.124,8.124,0,0,0,12.877-6.75Z\" transform=\"translate(0 21)\" fill=\"#231f20\"/>\r\n                            </svg>\r\n                            </span><span>{{lang 'common.account'}}</span>\r\n                            </a>\r\n\r\n                            {{else}}\r\n                            <a class=\"header-icon-container\" href=\"{{urls.auth.login}}\" aria-label=\"{{lang 'common.login'}}\">\r\n                               <span class=\"svg-container account-svg\"><svg xmlns=\"http://www.w3.org/2000/svg\" width=\"21\" height=\"24\" viewBox=\"0 0 21 24\">\r\n                              <path id=\"Path_116\" data-name=\"Path 116\" d=\"M10.5-9a6,6,0,0,0,6-6,6,6,0,0,0-6-6,6,6,0,0,0-6,6A6,6,0,0,0,10.5-9Zm2.377,2.25H8.123A8.124,8.124,0,0,0,0,1.373,1.625,1.625,0,0,0,1.625,3H19.376A1.622,1.622,0,0,0,21,1.373,8.124,8.124,0,0,0,12.877-6.75Z\" transform=\"translate(0 21)\" fill=\"#231f20\"/>\r\n                            </svg>\r\n                            </span> <span>Account</span>\r\n                            </a>\r\n                            {{/if}}\r\n                        </li>\r\n                        <li class=\"navUser-item navUser-item--cart\">\r\n                            <a class=\"header-icon-container\" data-cart-preview data-dropdown=\"cart-preview-dropdown\"\r\n                                data-options=\"align:right\" href=\"{{urls.cart}}\"\r\n                                aria-label=\"{{lang 'cart.nav_aria_label'}}\">\r\n                                <span class=\"svg-container cart-svg\"><svg xmlns=\"http://www.w3.org/2000/svg\" width=\"21\" height=\"24\" viewBox=\"0 0 21 24\">\r\n                                  <path id=\"Path_117\" data-name=\"Path 117\" d=\"M5.25-15.75A5.249,5.249,0,0,1,10.5-21a5.249,5.249,0,0,1,5.25,5.25v2.25h3A2.251,2.251,0,0,1,21-11.25V-1.5A4.5,4.5,0,0,1,16.5,3H4.5A4.5,4.5,0,0,1,0-1.5v-9.75A2.25,2.25,0,0,1,2.25-13.5h3ZM7.5-13.5h6v-2.25a3,3,0,0,0-3-3,3,3,0,0,0-3,3ZM6.375-9A1.122,1.122,0,0,0,7.5-10.125,1.122,1.122,0,0,0,6.375-11.25,1.122,1.122,0,0,0,5.25-10.125,1.122,1.122,0,0,0,6.375-9Zm8.25-2.25A1.122,1.122,0,0,0,13.5-10.125,1.122,1.122,0,0,0,14.625-9a1.122,1.122,0,0,0,1.125-1.125A1.122,1.122,0,0,0,14.625-11.25Z\" transform=\"translate(0 21)\" fill=\"#231f20\"/>\r\n                                </svg>\r\n                                </span>\r\n                                <span class=\"navUser-item-cartLabel\">{{lang 'common.cart'}}</span>\r\n                                <span class=\"countPill cart-quantity\"></span>\r\n                            </a>\r\n\r\n                            <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content\r\n                                aria-hidden=\"true\"></div>\r\n                        </li>\r\n                    </ul>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</header>\r\n\r\n<script>\r\n    const overlay = document.querySelector(\".overlay\");\r\n    const body = document.querySelector(\"body\");\r\n    const menuBtn = document.querySelector(\".menu-btn\");\r\n    const menuItems = document.querySelector(\".menu-items\");\r\n    const expandBtn = document.querySelectorAll(\".expand-btn\");\r\n    const grandChildrenMenus = document.querySelectorAll('.menu-children-btn')\r\n\r\n    function toggle() {\r\n        // disable overflow body\r\n        body.classList.toggle(\"overflow\");\r\n        // dark background\r\n        overlay.classList.toggle(\"overlay--active\");\r\n        // add open class\r\n        menuBtn.classList.toggle(\"open\");\r\n        menuItems.classList.toggle(\"open\");\r\n    }\r\n\r\n    menuBtn.addEventListener(\"click\", (e) => {\r\n        e.stopPropagation();\r\n        toggle();\r\n    });\r\n\r\n    window.onkeydown = function (event) {\r\n        const key = event.key; // const {key} = event; in ES6+\r\n        const active = menuItems.classList.contains(\"open\");\r\n        if (key === \"Escape\" && active) {\r\n            toggle();\r\n        }\r\n    };\r\n\r\n    document.addEventListener(\"click\", (e) => {\r\n        let target = e.target,\r\n            its_menu = target === menuItems || menuItems.contains(target),\r\n            its_hamburger = target === menuBtn,\r\n            menu_is_active = menuItems.classList.contains(\"open\");\r\n        if (!its_menu && !its_hamburger && menu_is_active) {\r\n            toggle();\r\n        }\r\n    });\r\n\r\n    // mobile menu expand\r\n    expandBtn.forEach((btn) => {\r\n        btn.addEventListener(\"click\", () => {\r\n            btn.classList.toggle(\"open\");\r\n            if(btn.id == 'catalogue_link'){\r\n                btn.parentNode.querySelector('#catalogue_items').classList.toggle(\"open\");\r\n            }\r\n        });\r\n    });\r\n    \r\n    grandChildrenMenus.forEach((gcMenu) => {\r\n        gcMenu.addEventListener(\"click\", ()=>{\r\n            gcMenu.classList.toggle(\"open\");\r\n            gcMenu.parentNode.querySelector('.mega-menu-grandchildren').classList.toggle(\"open\");\r\n        })\r\n    })\r\n    \r\n    //catalogue hover\r\n    const catalogue_link = document.getElementById('catalogue_link');\r\n    const catalogue_items = document.getElementById('catalogue_items');\r\n    \r\n    catalogue_link.addEventListener('mouseenter', addClassCatalogue);\r\n     catalogue_link.addEventListener('mouseleave', addClassCatalogue);\r\n    //   catalogue_link.addEventListener('click', addClassCatalogue);\r\n    function addClassCatalogue(){\r\n        catalogue_link.classList.toggle('open');\r\n        catalogue_items.classList.toggle('open');\r\n        console.log(\"asdasd\", catalogue_link.parentNode.querySelector('#catalogue_items'));\r\n    }\r\n\r\n\r\n\r\n// catalogue children's children hover\r\n    const non_first_index_menu_items = document.querySelectorAll('.menu-item-non-first-index');\r\n    const first_index_menu_item = document.querySelector('.index-first');\r\n\r\n    non_first_index_menu_items.forEach(non_first_index_menu_item => {\r\n      non_first_index_menu_item.addEventListener('mouseenter', () => {\r\n            first_index_menu_item.classList.add('hide-menu');\r\n      });\r\n      non_first_index_menu_item.addEventListener('mouseleave', () => {\r\n        first_index_menu_item.classList.remove('hide-menu');\r\n      });\r\n    });\r\n\r\n</script>\r\n","components/common/menu-navigation_":"<style>\r\n    .page-dropdown {\r\n        display: none;\r\n    }\r\n\r\n    .page-menu-item:hover .page-dropdown {\r\n        display: block;\r\n    }\r\n</style>\r\n\r\n<div class=\"navbar\">\r\n    <div class=\"menu-btn\">\r\n        <div class=\"menu-btn__lines\"></div>\r\n    </div>\r\n    <ul class=\"menu-items\">\r\n        <li>\r\n            <a href=\"#\" id=\"catalogue_link\" class=\"menu-item first-item expand-btn\">Catalogue</a>\r\n\r\n            <div class=\"mega-menu sample cat-1\" id =\"catalogue_items\">\r\n                <div class=\"sub-menu-1\">\r\n                    <ul class=\"menu-items menu-children\">\r\n                        {{#each categories}}\r\n                        {{#if children}}\r\n                        <li class=\"menu-item-list {{#if @index '!=' 0}}menu-item-non-first-index{{/if}}\">\r\n                            <a href=\"{{url}}\"\r\n                                class=\"menu-item first-item expand-btn menu-children-btn {{#if is_active}} activePage{{/if}}\">{{name}}</a>\r\n                            <div\r\n                                class=\"mega-menu mega-menu-grandchildren sample cat-two {{#if @index '!=' 0}}hide-menu non-index-first {{else}} index-first{{/if}}\">\r\n                                <div class=\"menu-grandchildren\">\r\n                                    <div class=\"col image-col\">\r\n                                        {{#if image}}\r\n                                        <section>\r\n                                            <a href=\"{{url}}\" aria-label=\"{{name}}\" class=\"img-wrapper\"><span\r\n                                                    class=\"img\"><img src=\"{{getImage image 'thumbnail'}}\"\r\n                                                        alt=\"{{name}}\" /></span></a>\r\n                                        </section>\r\n                                        {{/if}}\r\n                                    </div>\r\n                                    <div class=\"col menu-col\">\r\n                                        <div class=\"mega-menu sample mega-menu-greatgrandchildren\">\r\n                                            <div class=\"menu-title-container\">\r\n                                                <section>\r\n                                                    <div class=\"menu-title bold\">{{name}}</div>\r\n                                                </section>\r\n                                            </div>\r\n                                            <div class=\"content menu-greatgrandchildren-container\">\r\n                                                {{#each children}}\r\n                                                <div class=\"col\">\r\n                                                    <section>\r\n                                                        <a href=\"{{url}}\"\r\n                                                            class=\"menu-title {{#if is_active}} activePage{{/if}}\">{{name}}</a>\r\n                                                        <ul class=\"mega-links\">\r\n                                                            {{#each children}}\r\n                                                            <li><a href=\"{{url}}\"\r\n                                                                    class=\"menu-item {{#if is_active}} activePage{{/if}}\">{{name}}</a>\r\n                                                            </li>\r\n                                                            {{/each}}\r\n                                                        </ul>\r\n                                                    </section>\r\n                                                </div>\r\n                                                {{/each}}\r\n                                            </div>\r\n                                        </div>\r\n                                    </div>\r\n                                </div>\r\n                            </div>\r\n                        </li>\r\n                        {{else}}\r\n                        <li><a href=\"{{url}}\"\r\n                                class=\"menu-item first-item {{#if is_active}} activePage{{/if}}\">{{name}}</a></li>\r\n                        {{/if}}\r\n                        {{/each}}\r\n                    </ul>\r\n                </div>\r\n            </div>\r\n\r\n        </li>\r\n        {{#each pages}}\r\n        <li class=\"page-menu-item  {{#if children}}expand-btn{{/if}}\"><a href=\"{{#if children}}#{{else}}{{url}}{{/if}}\" aria-label=\"{{name}}\"\r\n                class=\"menu-item first-item {{#if name '==' ../page.title}} activePage{{/if}}\"> {{name}}</a>\r\n            {{#if children}}\r\n            <ul class=\"page-dropdown\">\r\n                {{#each children}}\r\n                <li class=\"page-sub-menu-item\">\r\n                    <a class=\"page-sub-menu-action {{#if name '==' ../page.title}} activePage{{/if}}\"\r\n                        href=\"{{url}}\">{{name}}</a></li>\r\n                {{/each}}\r\n            </ul>\r\n            {{/if}}\r\n        </li>\r\n        {{/each}}\r\n    </ul>\r\n</div>\r\n<div class=\"overlay\"></div>\r\n","components/common/modal/modal":"<div id=\"modal\" class=\"modal\" data-reveal data-prevent-quick-search-close>\n    {{> components/common/modal/modal-close-btn}}\n    <div class=\"modal-content\"></div>\n    <div class=\"loadingOverlay\"></div>\n</div>\n","components/common/modal/modal-close-btn":"<button class=\"modal-close\"\n        type=\"button\"\n        title=\"{{lang 'common.close'}}\"\n        {{additional_attr}}\n>\n    <span class=\"aria-description--hidden\">{{lang 'common.close'}}</span>\n    <span aria-hidden=\"true\">&#215;</span>\n</button>\n","components/common/navigation":"<div class=\"nav-container\">\r\n    <div class=\"container\">\r\n        <div class=\"navUser\">\r\n            <div class=\"notice-board-container\">\r\n                {{{region name=\"one_line_text_region--global\"}}}\r\n            </div>\r\n            <div class=\"organization-details-container\">\r\n                {{#if settings.phone_number}}\r\n                <div class=\"phone-number\">\r\n                    <a href=\"tel:{{settings.phone_number}}\" aria-label=\"organization contact number\">\r\n                    <span class=\"svg-container\"><svg xmlns=\"http://www.w3.org/2000/svg\" width=\"15.999\" height=\"16.001\"\r\n                            viewBox=\"0 0 15.999 16.001\">\r\n                            <path id=\"Path_113\" data-name=\"Path 113\"\r\n                                d=\"M15.975-1.906l-.727,3.15A.971.971,0,0,1,14.3,2,14.313,14.313,0,0,1,0-12.3a.971.971,0,0,1,.757-.952l3.15-.727a.981.981,0,0,1,1.118.566l1.454,3.391A.978.978,0,0,1,6.2-8.879L4.516-7.528A10.927,10.927,0,0,0,9.5-2.544l1.378-1.681a.972.972,0,0,1,1.14-.28l3.391,1.453A1.066,1.066,0,0,1,15.975-1.906Z\"\r\n                                transform=\"translate(0 14)\" fill=\"#fff\" />\r\n                        </svg></span>\r\n                    <p>{{settings.phone_number}}</p>\r\n                    </a>\r\n                </div>\r\n                {{/if}}\r\n                {{#if settings.address}}\r\n                <div class=\"address\">\r\n                    <span class=\"svg-container\"><svg xmlns=\"http://www.w3.org/2000/svg\" width=\"16\" height=\"12\"\r\n                            viewBox=\"0 0 16 12\">\r\n                            <path id=\"Path_114\" data-name=\"Path 114\"\r\n                                d=\"M8-3a2.389,2.389,0,0,1-1.474-.513L0-8.587V-1.5A1.5,1.5,0,0,0,1.5,0h13A1.5,1.5,0,0,0,16-1.5V-8.587L9.475-3.509A2.407,2.407,0,0,1,8-3ZM.509-9.459,7.14-4.3a1.4,1.4,0,0,0,1.721,0l6.631-5.159A1.412,1.412,0,0,0,16-10.5,1.5,1.5,0,0,0,14.5-12H1.5A1.5,1.5,0,0,0,0-10.5,1.319,1.319,0,0,0,.509-9.459Z\"\r\n                                transform=\"translate(0 12)\" fill=\"#fff\" />\r\n                        </svg>\r\n                    </span>\r\n                    <p>{{nl2br settings.address}}</p>\r\n                </div>\r\n                {{/if}}\r\n            </div>\r\n        </div>\r\n    </div>\r\n</div>\n","components/common/polyfill-script":"<script>\n    {{!--\n        Check for modern browser features, and load polyfills if browser does not appear to support features\n        we need.\n\n            Polyfill approach is all or nothing - please import needed polyfills in /assets/js/polyfills.js\n\n        Inspired by https://philipwalton.com/articles/loading-polyfills-only-when-needed/\n            --}}\n    function browserSupportsAllFeatures() {\n        return window.Promise\n            && window.fetch\n            && window.URL\n            && window.URLSearchParams\n            && window.WeakMap\n            // object-fit support\n            && ('objectFit' in document.documentElement.style);\n    }\n\n    function loadScript(src) {\n        var js = document.createElement('script');\n        js.src = src;\n        js.onerror = function () {\n            console.error('Failed to load polyfill script ' + src);\n        };\n        document.head.appendChild(js);\n    }\n\n    if (!browserSupportsAllFeatures()) {\n        loadScript('{{cdn 'assets/dist/theme-bundle.polyfills.js'}}');\n    }\n</script>\n","components/common/store-logo":"<a href=\"{{urls.home}}\" class=\"header-logo__link\" data-header-logo-link>\n    {{#if settings.store_logo.image}}\n        {{#if theme_settings.logo_size '===' 'original'}}\n        <img class=\"header-logo-image-unknown-size\"\n             src=\"{{getImage settings.store_logo.image 'logo_size'}}\"\n             alt=\"{{settings.store_logo.title}}\"\n             title=\"{{settings.store_logo.title}}\">\n        {{else}}\n        <div class=\"header-logo-image-container\">\n            <img class=\"header-logo-image\"\n                 src=\"{{getImage settings.store_logo.image 'logo_size'}}\"\n                 srcset=\"{{getImageSrcset1x2x settings.store_logo.image theme_settings.logo_size}}\"\n                 alt=\"{{settings.store_logo.title}}\"\n                 title=\"{{settings.store_logo.title}}\">\n        </div>\n        {{/if}}\n    {{else}}\n    <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\n    {{/if}}\n</a>\n","layout/base":"<!DOCTYPE html>\r\n<html class=\"no-js\" lang=\"{{ locale_name }}\">\r\n    <head>\r\n        <title>{{ head.title }}</title>\r\n        {{{ resourceHints }}}\r\n        {{{ head.meta_tags }}}\r\n        {{{ head.config }}}\r\n        {{#block \"head\"}} {{/block}}\r\n\r\n        <link href=\"{{ head.favicon }}\" rel=\"shortcut icon\">\r\n        <meta name=\"viewport\" content=\"width=device-width, initial-scale=1\">\r\n\r\n        <script>\r\n            {{!-- Change document class from no-js to js so we can detect this in css --}}\r\n            document.documentElement.className = document.documentElement.className.replace('no-js', 'js');\r\n        </script>\r\n\r\n        {{> components/common/polyfill-script }}\r\n        <script>window.consentManagerTranslations = `{{{langJson 'consent_manager'}}}`;</script>\r\n\r\n        {{!-- Load Lazysizes script ASAP so images will appear --}}\r\n        <script>\r\n            {{!-- Only load visible elements until the onload event fires, after which preload nearby elements. --}}\r\n            window.lazySizesConfig = window.lazySizesConfig || {};\r\n            window.lazySizesConfig.loadMode = 1;\r\n        </script>\r\n        <script async src=\"{{cdn 'assets/dist/theme-bundle.head_async.js' resourceHint='preload' as='script'}}\"></script>\r\n        \r\n        {{getFontsCollection font-display='block'}}\r\n        \r\n        <script async src=\"{{cdn 'assets/dist/theme-bundle.font.js' resourceHint='preload' as='script'}}\"></script>\r\n\r\n        {{{stylesheet '/assets/css/theme.css'}}}\r\n\r\n        {{{head.scripts}}}\r\n\r\n        {{~inject 'zoomSize' theme_settings.zoom_size}}\r\n        {{~inject 'productSize' theme_settings.product_size}}\r\n        {{~inject 'genericError' (lang 'common.generic_error')}}\r\n        {{~inject 'urls' urls}}\r\n        {{~inject 'secureBaseUrl' settings.secure_base_url}}\r\n        {{~inject 'cartId' cart_id}}\r\n        {{~inject 'template' template}}\r\n        {{~inject 'validationDictionaryJSON' (langJson 'validation_messages')}}\r\n        {{~inject 'validationFallbackDictionaryJSON' (langJson 'validation_fallback_messages')}}\r\n        {{~inject 'validationDefaultDictionaryJSON' (langJson 'validation_default_messages')}}\r\n        {{~inject 'carouselArrowAndDotAriaLabel' (lang 'carousel.arrow_and_dot_aria_label')}}\r\n        {{~inject 'carouselActiveDotAriaLabel' (lang 'carousel.active_dot_aria_label')}}\r\n        {{~inject 'carouselContentAnnounceMessage' (lang 'carousel.content_announce_message')}}\r\n    </head>\r\n    <body class=\"{{page_type}}\">\r\n        <svg data-src=\"{{cdn 'img/icon-sprite.svg'}}\" class=\"icons-svg-sprite\"></svg>\r\n\r\n        {{> components/common/header }}\r\n        {{> components/common/body }}\r\n        {{> components/common/footer }}\r\n\r\n        <script>window.__webpack_public_path__ = \"{{cdn 'assets/dist/'}}\";</script>\r\n        <script>\r\n            {{!-- Exported in app.js --}}\r\n            function onThemeBundleMain() {\r\n                window.stencilBootstrap(\"{{page_type}}\", {{jsContext}}).load();\r\n            }\r\n            \r\n            // custom script \r\n            var pathname = window.location.pathname;\r\n            var classname = pathname.substring(1);\r\n            var sanitizedPathname = classname.replace(/\\//g, '_');\r\n            document.body.classList.add(sanitizedPathname);\r\n\r\n        </script>\r\n        <script async defer src=\"{{cdn 'assets/dist/theme-bundle.main.js' resourceHint='preload' as='script'}}\" onload=\"onThemeBundleMain()\"></script>\r\n\r\n        {{{footer.scripts}}}\r\n    </body>\r\n</html>\r\n","pages/custom/page/about-us-layout":"{{#partial \"page\"}}\r\n\r\n{{> components/common/breadcrumbs breadcrumbs=breadcrumbs}}\r\n\r\n<section class=\"page about-us-layout\">\r\n    {{{region name=\"image_and_text_with_column_region\"}}}\r\n    {{{region name=\"about_us_multicolumn_region\"}}}\r\n    {{{region name=\"about_us_icon_with_text_region\"}}}\r\n    {{{region name=\"about_us_image_with_text_region\"}}}\r\n    {{{region name=\"about_us_brand_logos_region\"}}}\r\n    {{{region name=\"about_us_image_banner_region\"}}}\r\n\r\n</section>\r\n\r\n{{/partial}}\r\n\r\n{{> layout/base}}\n"}
