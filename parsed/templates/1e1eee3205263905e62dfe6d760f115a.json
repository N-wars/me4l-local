{"components/custom/featured-category":"<section class=\"featured-category-section featured-category-container\">\r\n    <div class=\"container\">\r\n        <div class=\"title-container text-center\">\r\n            <h2>Categories</h2>\r\n        </div>\r\n        <div class=\"categories-container grid grid-col-5-desktop grid-col-3-tablet grid-col-2-mobile\">\r\n            {{#each categories}}\r\n            <div class=\"categories-item grid-item\">\r\n                <div class=\"card-container card\">\r\n                    <div class=\"card-image-container\">\r\n                        <a href=\"{{url}}\" aria-label=\"{{name}}\">\r\n                            <img src=\"{{getImage image 'thumbnail'}}\" alt=\"{{name}}\">\r\n                        </a>\r\n                    </div>\r\n                    <div class=\"card-body\">\r\n                        <span class=\"bold\">{{name}}</span>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n            {{/each}}\r\n        </div>\r\n    </div>\r\n</section>\r\n"}
