{"components/common/menu-navigation_":"<style>\r\n    .page-dropdown {\r\n        display: none;\r\n    }\r\n\r\n    .page-menu-item:hover .page-dropdown {\r\n        display: block;\r\n    }\r\n</style>\r\n\r\n<div class=\"navbar\">\r\n    <div class=\"menu-btn\">\r\n        <div class=\"menu-btn__lines\"></div>\r\n    </div>\r\n    <ul class=\"menu-items\">\r\n        <li>\r\n            <a href=\"#\" id=\"catalogue_link\" class=\"menu-item first-item expand-btn\">Catalogue</a>\r\n\r\n            <div class=\"mega-menu sample cat-1\" id =\"catalogue_items\">\r\n                <div class=\"sub-menu-1\">\r\n                    <ul class=\"menu-items menu-children\">\r\n                        {{#each categories}}\r\n                        {{#if children}}\r\n                        <li class=\"menu-item-list {{#if @index '!=' 0}}menu-item-non-first-index{{/if}}\">\r\n                            <a href=\"{{url}}\"\r\n                                class=\"menu-item first-item expand-btn menu-children-btn {{#if is_active}} activePage{{/if}}\">{{name}}</a>\r\n                            <div\r\n                                class=\"mega-menu mega-menu-grandchildren sample cat-two {{#if @index '!=' 0}}hide-menu non-index-first {{else}} index-first{{/if}}\">\r\n                                <div class=\"menu-grandchildren\">\r\n                                    <div class=\"col image-col\">\r\n                                        {{#if image}}\r\n                                        <section>\r\n                                            <a href=\"{{url}}\" aria-label=\"{{name}}\" class=\"img-wrapper\"><span\r\n                                                    class=\"img\"><img src=\"{{getImage image 'thumbnail'}}\"\r\n                                                        alt=\"{{name}}\" /></span></a>\r\n                                        </section>\r\n                                        {{/if}}\r\n                                    </div>\r\n                                    <div class=\"col menu-col\">\r\n                                        <div class=\"mega-menu sample mega-menu-greatgrandchildren\">\r\n                                            <div class=\"menu-title-container\">\r\n                                                <section>\r\n                                                    <div class=\"menu-title bold\">{{name}}</div>\r\n                                                </section>\r\n                                            </div>\r\n                                            <div class=\"content menu-greatgrandchildren-container\">\r\n                                                {{#each children}}\r\n                                                <div class=\"col\">\r\n                                                    <section>\r\n                                                        <a href=\"{{url}}\"\r\n                                                            class=\"menu-title {{#if is_active}} activePage{{/if}}\">{{name}}</a>\r\n                                                        <ul class=\"mega-links\">\r\n                                                            {{#each children}}\r\n                                                            <li><a href=\"{{url}}\"\r\n                                                                    class=\"menu-item {{#if is_active}} activePage{{/if}}\">{{name}}</a>\r\n                                                            </li>\r\n                                                            {{/each}}\r\n                                                        </ul>\r\n                                                    </section>\r\n                                                </div>\r\n                                                {{/each}}\r\n                                            </div>\r\n                                        </div>\r\n                                    </div>\r\n                                </div>\r\n                            </div>\r\n                        </li>\r\n                        {{else}}\r\n                        <li><a href=\"{{url}}\"\r\n                                class=\"menu-item first-item {{#if is_active}} activePage{{/if}}\">{{name}}</a></li>\r\n                        {{/if}}\r\n                        {{/each}}\r\n                    </ul>\r\n                </div>\r\n            </div>\r\n\r\n        </li>\r\n        {{#each pages}}\r\n        <li class=\"page-menu-item  {{#if children}}expand-btn{{/if}}\"><a href=\"{{#if children}}#{{else}}{{url}}{{/if}}\" aria-label=\"{{name}}\"\r\n                class=\"menu-item first-item {{#if name '==' ../page.title}} activePage{{/if}}\"> {{name}}</a>\r\n            {{#if children}}\r\n            <ul class=\"page-dropdown\">\r\n                {{#each children}}\r\n                <li class=\"page-sub-menu-item\">\r\n                    <a class=\"page-sub-menu-action {{#if name '==' ../page.title}} activePage{{/if}}\"\r\n                        href=\"{{url}}\">{{name}}</a></li>\r\n                {{/each}}\r\n            </ul>\r\n            {{/if}}\r\n        </li>\r\n        {{/each}}\r\n    </ul>\r\n</div>\r\n<div class=\"overlay\"></div>\r\n"}
